    <asp:Panel ID="panelCambiarContrasena" CssClass="modal panelContrasena" runat="server" role="dialog">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="color-line"></div>
                                <div class="modal-header text-center">
                                    <h4 class="modal-title">Cambiar Contraseña</h4>
                                     </div>
                                <div class="modal-body">
                                   
                                    <div class="form-group" style="margin-bottom:5px;">
                                        <div class="col-lg-4">
                                            <label>Usuario:</label>
                                        </div>
                                        <div class="col-lg-8">
                                            <asp:TextBox ID="txtUsuario" ReadOnly="true" CssClass="form-control" runat="server"></asp:TextBox> 
                                          <asp:TextBox ID="txtIDUsuario"  style="display:none;" CssClass="form-control" runat="server"></asp:TextBox>   
                                             
                                        </div> 
                                        <div style="clear:both;"></div>
                                      </div>  
                                    
                                    <div class="form-group" style="margin-bottom:5px;">
                                        <div class="col-lg-4">
                                            <label>Constraseña Actual:</label>
                                        </div>
                                        <div class="col-lg-8">
                                            <asp:TextBox ID="txtContraseña" TextMode="Password" CssClass="form-control" runat="server"></asp:TextBox> 
                                            <asp:RequiredFieldValidator ID="RequiredFieldValidator4" runat="server" 
                                                ErrorMessage="Ingresar contraseña" ControlToValidate="txtContraseña" 
                                                CssClass="error" Display="Dynamic"
                                                SetFocusOnError="True" ValidationGroup="concepto"></asp:RequiredFieldValidator>
                                        </div> 
                                        <div style="clear:both;"></div>
                                      </div>

                                      <div class="form-group" style="margin-bottom:5px;">
                                        <div class="col-lg-4">
                                            <label>Nueva Contraseña:</label>
                                        </div>
                                        <div class="col-lg-8">
                                           <asp:TextBox ID="txtNuevaContraseña" TextMode="Password" CssClass="form-control" runat="server"></asp:TextBox> 
                                            <asp:RequiredFieldValidator ID="RequiredFieldValidator3" runat="server" 
                                                ErrorMessage="Ingresar nueva contraseña" ControlToValidate="txtNuevaContraseña" 
                                                CssClass="error" Display="Dynamic" SetFocusOnError="True" 
                                                ValidationGroup="concepto"  ></asp:RequiredFieldValidator>
                                        </div> 
                                        <div style="clear:both;"></div>
